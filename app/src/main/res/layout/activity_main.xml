<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/image_11"
            android:layout_width="match_parent"
            android:layout_height="808dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="-24dp"
            android:background="@drawable/image_11" />

        <EditText
            android:id="@+id/e_mail2"
            android:layout_width="240dp"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="74dp"
            android:layout_marginTop="409dp"
            android:background="@null"
            android:fontFamily="@font/font1"
            android:gravity="center_vertical"
            android:hint="@string/e_mail"
            android:inputType="textEmailAddress"
            android:textColor="#B7A8A8"
            android:textSize="32sp" />

        <ImageView
            android:id="@+id/logo1"
            android:layout_width="315dp"
            android:layout_height="243dp"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="39dp"
            android:layout_alignParentTop="true"
            android:layout_marginTop="71dp"
            android:scaleType="center"
            android:background="@drawable/logo1"
            />

        <ImageView
            android:id="@+id/line_1"
            android:layout_width="250dp"
            android:layout_height="1dp"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="72dp"
            android:layout_alignParentTop="true"
            android:layout_marginTop="444dp"
            android:background="@drawable/line_1"
            />

        <ImageView
            android:id="@+id/line_2"
            android:layout_width="250dp"
            android:layout_height="1dp"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="72dp"
            android:layout_alignParentTop="true"
            android:layout_marginTop="498dp"
            android:background="@drawable/line_1"
            />

        <ImageView
            android:id="@+id/penicon1"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="322dp"
            android:layout_alignParentTop="true"
            android:layout_marginTop="409dp"
            android:background="@drawable/penicon1"
            />

        <ImageView
            android:id="@+id/rectangle_1"
            android:layout_width="250dp"
            android:layout_height="39dp"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="72dp"
            android:layout_alignParentTop="true"
            android:layout_marginTop="524dp"
            android:background="@drawable/rectangle_2"
            />

        <View
            android:id="@+id/rectangle_2"
            android:layout_width="250dp"
            android:layout_height="39dp"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="72dp"
            android:layout_alignParentTop="true"
            android:layout_marginTop="569dp"
            android:background="@drawable/rectangle_2"
            />

        <EditText
            android:id="@+id/e_mail"
            android:layout_width="240dp"
            android:layout_height="34dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="74dp"
            android:layout_marginTop="409dp"
            android:fontFamily="@font/font1"
            android:gravity="center_vertical"
            android:inputType="textEmailAddress"
            android:hint="@string/e_mail"
            android:textColor="#B7A8A8"
            android:textSize="32sp"
            android:background="@null"
            />

        <EditText
            android:id="@+id/password"
            android:layout_width="240dp"
            android:layout_height="34dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="74dp"
            android:layout_marginTop="462dp"
            android:background="@null"
            android:fontFamily="@font/font1"
            android:gravity="center_vertical"
            android:hint="Password"
            android:inputType="textPassword"
            android:textColor="#B7A8A8"
            android:textSize="32sp" />

        <ImageView
            android:id="@+id/penicon2"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="322dp"
            android:layout_alignParentTop="true"
            android:layout_marginTop="462dp"
            android:background="@drawable/penicon1"
            />

        <TextView
            android:id="@+id/log_in"
            android:layout_width="90dp"
            android:layout_height="37dp"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="152dp"
            android:layout_alignParentTop="true"
            android:layout_marginTop="525dp"
            android:text="@string/log_in"
            android:fontFamily="@font/font1"
            android:textSize="32sp"
            android:textColor="#757575"
            android:gravity="center"
            android:clickable="true"
            android:focusable="true"
            />

        <Button
            android:id="@+id/singGoogle"
            android:layout_width="185dp"
            android:layout_height="37dp"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="105dp"
            android:layout_alignParentTop="true"
            android:layout_marginTop="575dp"
            android:background="@drawable/group1"
            />

        <TextView
            android:id="@+id/register"
            android:layout_width="106dp"
            android:layout_height="20dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="100dp"
            android:layout_marginTop="630dp"
            android:clickable="true"
            android:focusable="true"
            android:fontFamily="@font/font1"
            android:gravity="top"
            android:text="@string/register"
            android:textColor="#757575"
            android:textSize="24sp" />

        <TextView
            android:id="@+id/find_password"
            android:layout_width="106dp"
            android:layout_height="20dp"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="203dp"
            android:layout_alignParentTop="true"
            android:layout_marginTop="630dp"
            android:text="@string/find_password"
            android:fontFamily="@font/font1"
            android:textSize="24sp"
            android:textColor="#757575"
            android:gravity="top"
            android:clickable="true"
            android:focusable="true"
            />

    </FrameLayout>

</LinearLayout>